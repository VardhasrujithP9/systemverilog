class packet;
  rand int  a;
  rand byte b;
 
  constraint c1{a>=90;a<=150;b>=100;b<=500;}
  constraint c2{a<120;}
endclass
//----------------testcase----------------------//
  module test;
    packet p1,p2;
    initial
     begin
       p1=new();
       repeat(25)
         begin
           p1.randomize();
           $display("a=%d,b=%d",p1.a,p1.b);
         end
     end
  endmodule
       
       